    "rimraf": "^3.0.1",
    "sass-loader": "^8.0.2",
    "size-plugin": "^2.0.1",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2"
