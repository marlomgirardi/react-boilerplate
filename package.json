{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "This is my personal React boilerplate with everything that I need.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --hot --config ./internals/webpack/webpack.dev.js",

    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack --config ./internals/webpack/webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marlomgirardi/react-boilerplate.git"
  },
